<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0" />
  <template class="MainLeaflet" parent="AdwBin">
    <child>
      <object class="AdwLeaflet" id="leaflet">
        <property name="homogeneous">0</property>
        <property name="can-navigate-back">1</property>
        <property name="transition-type">over</property>
        <property name="visible-child">left_box</property>
        <signal name="notify::folded" handler="on_leaflet_folded" />
        <child>
          <object class="AdwLeafletPage" id="left_page">
            <property name="child">
              <object class="GtkBox" id="left_box">
                <property name="hexpand">0</property>
                <property name="orientation">vertical</property>
                <property name="width-request">360</property>
                <property name="height-request">100</property>
              </object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwLeafletPage">
            <property name="navigatable">0</property>
            <property name="child">
              <object class="GtkSeparator"></object>
            </property>
          </object>
        </child>
        <child>
          <object class="AdwLeafletPage" id="right_page">
            <property name="child">
              <object class="GtkBox" id="right_box">
                <property name="orientation">vertical</property>
              </object>
            </property>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
